	<entry key="version">Version ${version}</entry>
	
	<entry key="border">----------------------------------------</entry>
	
	<entry key="greeting">MicroDebug - Copyright (C) 2011-2012 Christian RÃ¶sch</entry>
	<entry key="welcome">Welcome! Please type 'help' for a list of valid commands</entry>

